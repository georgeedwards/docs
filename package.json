{
  "name": "Nativescript_Docs",
  "version": "1.0.0",
  "description": "Docs site for nativescript",
  "main": "server/app.js",
  "private": true,
  "scripts": {
    "develop": "concurrently \"tsc -w -p ./server\" \"tsc -w -p ./client\" \"nodemon ./server/bin/www\" ",
    "forever": "tsc -p ./server && forever start ./server/bin/www --watch",
    "twc:client:w": "tsc -w -p ./client",
    "twc:server:w": "tsc -w -p ./server",
    "bundle:prod": "node tools/builder.js --prod",
    "postinstall": "tsc -p ./server",
    "start": "node ./server/bin/www",
    "ngc": "ngc -p client/tsconfig-aot.json",
    "lint": "tslint -c tslint.json \"client/**/*.ts\"",
    "rollup": "rollup -c rollup-config.js",
    "build": "npm run ngc && npm run rollup && npm run tsc --target es5 --allowJs public/assets/js/build.es2015.js --out public/assets/js/bundle.js"
  },
  "engines": {
    "node": "6.7.0"
  },
  "author": "George Edwards",
  "license": "MIT",
  "dependencies": {
    "@types/auth0-lock": "^10.0.20",
    "@types/body-parser": "0.0.33",
    "@types/elasticsearch": "0.0.7",
    "@types/es6-shim": "^0.31.32",
    "@types/express": "4.0.32",
    "@types/express-jwt": "0.0.33",
    "@types/express-validator": "2.20.33",
    "@types/jsonwebtoken": "7.1.32",
    "@types/mongoose": "4.5.37",
    "@types/morgan": "1.7.32",
    "@types/node-schedule": "0.0.34",
    "@types/request": "0.0.31",
    "@types/serve-favicon": "2.2.28",
    "auth0-lock": "^10.4.0",
    "body-parser": "1.15.2",
    "cookie-parser": "1.4.3",
    "debug": "2.2.0",
    "elasticsearch": "^11.0.1",
    "es6-shim": "^0.35.1",
    "express": "4.13.4",
    "express-jwt": "5.0.0",
    "express-validator": "2.20.10",
    "forever": "0.15.2",
    "jsonwebtoken": "7.1.9",
    "mongoose": "4.6.1",
    "morgan": "1.7.0",
    "node-schedule": "1.1.1",
    "request": "2.75.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "@angular/common": "2.0.2",
    "@angular/compiler": "2.0.2",
    "@angular/compiler-cli": "0.6.4",
    "@angular/core": "2.0.2",
    "@angular/forms": "2.0.2",
    "@angular/http": "2.0.2",
    "@angular/platform-browser": "2.0.2",
    "@angular/platform-browser-dynamic": "2.0.2",
    "@angular/platform-server": "2.0.2",
    "@angular/router": "3.0.2",
    "@types/es6-promise": "0.0.32",
    "@types/jasmine": "2.2.33",
    "angular2-in-memory-web-api": "0.0.21",
    "angular2-jwt": "0.1.22",
    "codelyzer": "^1.0.0-beta.1",
    "concurrently": "3.1.0",
    "core-js": "2.4.1",
    "nodemon": "1.9.1",
    "reflect-metadata": "0.1.3",
    "rollup": "^0.36.1",
    "rollup-plugin-commonjs": "^5.0.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.38",
    "systemjs-builder": "0.15.26",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "yargs": "6.0.0",
    "zone.js": "0.6.23"
  }
}

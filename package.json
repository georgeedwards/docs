{
  "name": "Nativescript_Docs",
  "version": "1.0.0",
  "description": "Docs site for nativescript",
  "main": "server/app.js",
  "private": true,
  "scripts": {
    "develop": "concurrently \"tsc -w -p ./server\" \"tsc -w -p ./client\" \"nodemon ./server/bin/www\" ",
    "forever": "tsc -p ./server && forever start ./server/bin/www --watch",
    "twc:client:w": "tsc -w -p ./client",
    "twc:server:w": "tsc -w -p ./server",
    "bundle:prod": "node tools/builder.js --prod",
    "postinstall": "tsc -p ./server",
    "start": "node ./server/bin/www"
  },
  "engines": {
    "node": "6.7.0"
  },
  "author": "George Edwards",
  "license": "MIT",
  "dependencies": {
    "@types/body-parser": "0.0.33",
    "@types/elasticsearch": "0.0.7",
    "@types/express": "4.0.32",
    "@types/express-jwt": "0.0.33",
    "@types/express-validator": "2.20.33",
    "@types/jsonwebtoken": "7.1.32",
    "@types/mongoose": "4.5.37",
    "@types/morgan": "1.7.32",
    "@types/node-schedule": "0.0.34",
    "@types/request": "0.0.31",
    "@types/serve-favicon": "2.2.28",
    "body-parser": "1.15.2",
    "cookie-parser": "1.4.3",
    "debug": "2.2.0",
    "elasticsearch-browser": "11.0.1",
    "express": "4.13.4",
    "express-jwt": "5.0.0",
    "express-validator": "2.20.10",
    "forever": "0.15.2",
    "jsonwebtoken": "7.1.9",
    "mongoose": "4.6.4",
    "morgan": "1.7.0",
    "node-schedule": "1.2.0",
    "request": "2.75.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/core": "2.0.1",
